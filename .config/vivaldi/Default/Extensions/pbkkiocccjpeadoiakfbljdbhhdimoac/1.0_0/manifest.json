{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "background": {
         "persistent": false,
         "scripts": [ "js/background.js" ]
      },
      "css": [ "css/styles.css" ],
      "js": [ "js/jquery.js", "js/background.js" ],
      "matches": [ "https://www.youtube.com/*" ]
   } ],
   "description": "Youtube thumbnail resizer brings back 6 thumbnails in row instead of 4",
   "icons": {
      "128": "icon_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Youtube thumbnail resizer by iAlen",
   "permissions": [ "declarativeContent", "activeTab", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
